{
	"build": {
		"jsFile": "wink-VERSION-PROFILE-TARGET.js",
		"cssFile": "wink-VERSION-PROFILE-TARGET.css",
		"version": "1.4.4",
		"defaultLocaleList": [ "en_EN" ]
	},
	"profiles": [
		{
			"name": "all",
			"modules": [
				// CORE
				"core",
				// FEATURE EXTENDED
				"_base/_feat/featapi", "_base/_feat/featgraphic",
				// JSON
				"_base/json/jsonextended",
				// API
				"api/geolocation", "api/storage", "api/safaridb", "api/gearsdb",
				// FX
				"fx/3dfx", "fx/animation",
				// MATH
				"math/geometric", "math/matrix",
				// NET
				"net/cssloader", "net/imagesloader", "net/jsloader", "net/smartloader", "net/tracking",
				// MULTIMEDIA
				"mm/audioplayer", "mm/videoplayer", "mm/mediawheel",
				// UX
				"ux/history", "ux/dnd", "ux/gesture", "ux/gesturerecognizer", "ux/inertia", "ux/movementtracker", "ux/search", "ux/window",
				// UI
				"ui/layout/accordion", "ui/layout/fixedlayout", "ui/layout/scroller", "ui/layout/slidingpanels", "ui/layout/tabcontainer", "ui/layout/modalwindow", "ui/layout/flippage", "ui/layout/windows", "ui/form/input", "ui/xy/carousel",
				"ui/xy/colorpicker", "ui/xy/datepicker", "ui/xy/menu", "ui/xy/newsticker", "ui/xy/popup", "ui/xy/progressbar", "ui/xy/slideshow", "ui/xy/spinner", "ui/xy/cspinner", "ui/xy/trace",
				"ui/xy/togglebutton", "ui/xyz/wall", "ui/xyz/coverflow", "ui/xyz/cube", "ui/xyz/flipdisc", "ui/xyz/opener", "ui/xyz/tagcloud",
				// PLUGINS
				"plugins/asyncpanels", "plugins/simplestorage", "plugins/featdetectstorage", "plugins/gestures", "plugins/motion", "plugins/scrolltoload", "plugins/scrolltorefresh",
				"plugins/sharingwheel", "plugins/topbottom", "plugins/visualfeedback", "plugins/winkjquery"
			],
			"css": [
				"_themes/wink.css",
				"_themes/default/css/wink.default.css",
				"_themes/default/css/wink.default.buttons.css",
				"_themes/default/css/wink.default.icons.css"
			]
		},
		{
			"name": "core",
			"modules": [
				"core"
			],
			"css": [
				"_themes/wink.css",
				"_themes/default/css/wink.default.css",
				"_themes/default/css/wink.default.buttons.css",
				"_themes/default/css/wink.default.icons.css"
			]
		},
		{
			"name": "easycaching",
			"modules": [
				"_cache"
			]
		}
	]
}